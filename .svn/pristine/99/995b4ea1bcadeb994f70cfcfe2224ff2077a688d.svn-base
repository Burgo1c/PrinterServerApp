<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<!-- DO NOT CHANGE FILE LAYOUT -->
	<!-- DO NOT CHANGE KEY NAME -->
	<section>
		<!--LOG PATH'S-->
		<key name="LOG_PATH">../logs/</key>
		<key name="ERROR_LOG">error.log</key>
		<key name="HTTP_ERROR_LOG">http-errors.log</key>
		<key name="PRINT_ERROR_LOG">print-errors.log</key>

		<!--API ENDPOINTS-->
		<!--& => &amp;-->
		<key name="PRINT_LIST_API">http://164.70.117.102/ueda/php/pdfApi.php?Type=printList</key>
		<key name="RECEIPT_PDF_API">http://164.70.117.102/ueda/php/pdfApi.php?Type=receiptPdf&amp;order_no=</key>
		<key name="LABEL_PDF_API">http://164.70.117.102/ueda/php/pdfApi.php?Type=deliverySlip&amp;order_no=</key>
		<key name="SHUKA_IRAI_PDF_API">http://164.70.117.102/ueda/php/pdfApi.php?Type=hikaePdf&amp;order_no=</key>
		<key name="NOUHIN_PDF_API">http://164.70.117.102/ueda/php/pdfApi.php?Type=nouhinPdf&amp;order_no=</key>
		<key name="DENPYO_PDF_API">http://164.70.117.102/ueda/php/pdfApi.php?Type=denpyoPdf&amp;order_no=</key>
		<key name="ORDER_UPDATE_API">http://164.70.117.102/ueda/php/pdfApi.php?Type=updateReportPrint&amp;order_no=</key>
		<key name="PRINT_HISTORY_API">http://164.70.117.102/ueda/php/pdfApi.php?Type=printHistory</key>

		<!--PRINT PDF CHECK-->
		<!--TRUE or FALSE-->
		<key name="RECEIPT_PDF_PRINT">false</key>
		<key name="LABEL_PDF_PRINT">TRUE</key>
		<key name="SHUKA_IRAI_PDF_PRINT">TRUE</key>
		<key name="NOUHIN_PDF_PRINT">TRUE</key>
		<key name="DENPYO_PDF_PRINT">TRUE</key>
		
		<!--API RETRY TIMES-->
		<key name="API_RETRY">3</key>

		<!--TEMP OUTPUT PATH-->
		<key name="FILE_PATH">../temp/</key>

		<!--SLEEP・DELAY TIME (in miliseconds)-->
		<key name="SLEEP_TIME">5000</key>
	</section>
</configuration>
